file_IN="./studenti.json"



file_OUT=$file_IN".t"
echo "[" > $file_OUT
while read line
do
    line=${line// \"/\"}
    line=${line//\{\"\$oid\"\:/ObjectId(}
    line=${line//\}\,\"/\)\,\"}
    line=${line//\:/\: }
    line=${line//\,/\, }
    line=${line//\{/\{ }
    line=${line//\}/ \}\,}
    
    echo $line
done < $file_IN >> $file_OUT

echo "]" >> $file_OUT

mv ./$file_IN{.t,}